{
  "name": "ts-discord-bot-template",
  "version": "1.0.0",
  "description": "A Discord bot template using discord.js, discord.js-commando, and TypeScript. Supports auto-compilation.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jacksonrakena/ts-discord-bot-template.git"
  },
  "scripts": {
    "debug": "nodemon -e js,ts --watch src --exec \"npm run build:ts && npm run start:app\"",
    "build:ts": "tsc",
    "start:app": "node dist/index.js",
    "start": "npm run debug"
  },
  "author": "abyssal512",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^11.5.1",
    "discord.js-commando": "^0.10.0",
    "sqlite": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "^12.20.13",
    "@types/puppeteer": "^5.4.4",
    "nodemon": "^2.0.7",
    "puppeteer": "^10.2.0",
    "tsc": "^2.0.3",
    "typescript": "^4.4.3"
  }
}
